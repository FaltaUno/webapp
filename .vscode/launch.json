  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}",
      "name": "Next: Node",
      "port": 9229,
      "console": "integratedTerminal"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Next: Chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceRoot}"
    }
  ],
  "compounds": [
    {
      "name": "Next: Full",
      "configurations": ["Next: Node", "Next: Chrome"]
    }
  ]
}
