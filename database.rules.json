{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "users": {
    	".indexOn": ["available"],
      "$uid":{
        "matches":{
          ".indexOn": ["date"]
        }
      }
    },
    "matches": {
      ".indexOn": ["date"],
      "$mid":{
        ".read": true
      }
    }
  }
}
